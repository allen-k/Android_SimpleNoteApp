    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ca.uoit.AllenKaganovsky.mobile_a2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ca.uoit.AllenKaganovsky.mobile_a2.NewNote"
            android:label="@string/title_activity_new_note" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ca.uoit.AllenKaganovsky.mobile_a2.MainActivity" />
        </activity>
        <activity
            android:name="ca.uoit.AllenKaganovsky.mobile_a2.EditNote"
            android:label="@string/title_activity_edit_note" >
